{
  "name": "gallery",
  "version": "1.0.0",
  "description": "Gallery webapp",
  "license": "MIT",
  "engines": {
    "node": ">=14.x",
    "yarn": "1.x.x"
  },
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.js .",
    "prettier": "prettier --write",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prathusingh/gallery"
  },
  "keywords": [
    "gallery",
    "react"
  ],
  "homepage": "https://github.com/artsy/eigen#readme",
  "author": "Prathu Singh",
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "add": "^2.0.6",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "yarn": "^1.22.18"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn prettier",
      "yarn lint"
    ],
    "*.@(json|md|js)": [
      "yarn prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
