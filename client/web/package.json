{
  "name": "wink-sassy",
  "version": "0.0.1",
  "description": "Wink sassy webapp",
  "engines": {
    "node": "14.x",
    "npm": "7.x",
    "yarn": "1.x"
  },
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.js .",
    "prettier": "prettier --write",
    "type-check": "yarn tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prathusingh/wink-sassy"
  },
  "keywords": [
    "wink-sassy",
    "react"
  ],
  "homepage": "https://github.com/artsy/eigen#readme",
  "author": "Prathu Singh",
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn prettier",
      "yarn lint"
    ],
    "*.@(json|md|js)": [
      "yarn prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
